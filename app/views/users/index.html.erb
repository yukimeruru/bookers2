<div class="container">
  <div class="row">
    <div class="col-3">
      
      
      <h2>Users</h2>
      <p>image</p><p>name</p>
      <% @users.each do |user| %>

      <div>
        <p><%= attachment_image_tag user, :profile_image %></p>
        <p><%= user.name %></p>
        <p><%= link_to "Show", user_path(user.id) %></p>
      </div>
      <% end %>
    
    </div>
  </div>
</div>